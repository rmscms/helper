{
    "name": "rmscms/helper",
    "description": "A helper package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sharif",
            "email": "msharif.ahrari@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RMS\\Helper\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RMS\\Helper\\HelperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
